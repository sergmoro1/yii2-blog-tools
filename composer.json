{
    "name": "sergmoro1/yii2-blog-tools",
    "version": "1.0.0",
    "description": "Blog tools. Posts, Tags, Rubrics, Comments backend management. Tag cloud, Rubric Tree (Nested Set), Ricent Comments widgets.",
    "keywords": [
        "yii2",
        "blog", "tag",
        "rubric", "comment"
    ],
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Morozov",
            "email": "sergmoro1@ya.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "creocoder/yii2-nested-sets": "^0.9.0",
        "vova07/yii2-imperavi-widget" : "*",
        "notgosu/yii2-meta-tag-module": "dev-master",
        "sergmoro1/yii2-lookup" : "dev-master",
        "sergmoro1/yii2-byone-uploader" : "dev-master"
    },
    "autoload": {
        "psr-4": {
            "sergmoro1\\blog\\": "src"
        }
    }
}
